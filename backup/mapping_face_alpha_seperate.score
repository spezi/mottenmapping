\"float\",\"MIN\":0.0,\"MAX\":40.0,\"DEFAULT\":18.0},\n        {\"NAME\":\"Filling\",\"TYPE\":\"float\",\"MIN\":1.0,\"MAX\":6.0,\"DEFAULT\":3.2},\n\n        {\"NAME\":\"ShapeWarp\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":2.0,\"DEFAULT\":0.6},\n        {\"NAME\":\"WarpFlow\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":2.0,\"DEFAULT\":1.0},\n\n        {\"NAME\":\"TwistFlow\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":3.0,\"DEFAULT\":1.0},\n        {\"NAME\":\"TwistPoles\",\"TYPE\":\"float\",\"MIN\":1.0,\"MAX\":12.0,\"DEFAULT\":4.0},\n\n        {\"NAME\":\"FlowSpeed\",\"TYPE\":\"float\",\"MIN\":0.05,\"MAX\":3.0,\"DEFAULT\":1.0},\n\n        {\"NAME\":\"density\",\"TYPE\":\"float\",\"MIN\":0.5,\"MAX\":12.0,\"DEFAULT\":3.5},\n        {\"NAME\":\"Glow\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":2.0,\"DEFAULT\":1.0},\n\n        {\"NAME\":\"ChaosMotion\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":2.0,\"DEFAULT\":0.7},\n        {\"NAME\":\"MicroChaos\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":1.0,\"DEFAULT\":0.4},\n        {\"NAME\":\"NoiseOverlay\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":1.5,\"DEFAULT\":0.5},\n        {\"NAME\":\"OverlayTwist\",\"TYPE\":\"float\",\"MIN\":0.0,\"MAX\":2.0,\"DEFAULT\":1.0}\n    ]\n}*/\n\n#define PI 3.141592653589793\n\nfloat hash21(vec2 p) {\n    p = fract(p * vec2(123.34, 345.45));\n    p += dot(p, p + 34.345);\n    return fract(p.x * p.y);\n}\n\nfloat softNoise(vec2 p){\n    vec2 i = floor(p);\n    vec2 f = fract(p);\n    f = f*f*(3.0 - 2.0*f);\n    float a = hash21(i);\n    float b = hash21(i+vec2(1.0,0.0));\n    float c = hash21(i+vec2(0.0,1.0));\n    float d = hash21(i+vec2(1.0,1.0));\n    return mix(mix(a,b,f.x), mix(c,d,f.x), f.y);\n}\n\nmat2 rot(float a){\n    float c = cos(a), s = sin(a);\n    return mat2(c,-s,s,c);\n}\n\nvoid main() {\n    vec2 uv = (gl_FragCoord.xy - 0.5 * RENDERSIZE.xy) / RENDERSIZE.y;\n    float t = TIME * 0.42 * FlowSpeed;\n\n    //----------------------------------------------------\n    // ChaosMotion - mouvement global organique\n    //----------------------------------------------------\n    vec2 chaos = vec2(\n        softNoise(uv*3.0 + t*0.5),\n        softNoise(uv*3.0 - t*0.6)\n    );\n    uv += (chaos - 0.5) * ChaosMotion * 0.3;\n\n    //----------------------------------------------------\n    // 1. ShapeWarp\n    //----------------------------------------------------\n    float dn = softNoise(uv * (2.5 + ShapeWarp*3.0) + t);\n    uv += vec2(\n        sin(uv.y * 3.0 + dn*2.5 + t)*0.18*ShapeWarp,\n        sin(uv.x * 3.0 - dn*2.2 - t)*0.18*ShapeWarp\n    );\n\n    //----------------------------------------------------\n    // 2. TwistFlow\n    //----------------------------------------------------\n    float localN = softNoise(uv * (1.5 + ShapeWarp*1.8) + t*0.7);\n    float radius = length(uv) + 0.001;\n    float singleTwistAngle = sin(radius * 6.0 - t*1.2) * 0.5 * TwistFlow + localN * TwistFlow * 0.6;\n\n    //----------------------------------------------------\n    // 3. Twist multipolaire\n    //----------------------------------------------------\n    float angle = atan(uv.y, uv.x);\n    float polarComponent = sin(angle * TwistPoles * 2.0 + t*1.5) *\n                           cos(radius * TwistPoles * 1.3 - t*0.8) *\n                           0.35 * TwistFlow;\n    float totalTwist = singleTwistAngle + polarComponent;\n    uv = rot(totalTwist) * uv;\n\n    //----------------------------------------------------\n    // 4. WarpFlow\n    //----------------------------------------------------\n    uv += vec2(\n        sin(uv.y*2.2 + t*1.5)*0.12*WarpFlow,\n        cos(uv.x*2.8 - t*1.2)*0.12*WarpFlow\n    );\n\n    //----------------------------------------------------\n    // 5. Filaments field (densitÃ© + micro chaos)\n    //----------------------------------------------------\n    float nField = softNoise(uv * Filling * 1.4 + t*0.3);\n    float swirl = sin(uv.x * Stripe + nField*5.0 + t*1.1) +\n                  cos(uv.y * Stripe*0.6 - nField*4.0 + t*0.7);\n    swirl += softNoise(uv*density + t)*2.0;\n    swirl += softNoise(uv*8.0 + t*2.0) * MicroChaos; // micro chaos\n    float fil = abs(sin(swirl)) * 0.6 + (nField * 0.4);\n\n    //----------------------------------------------------\n    // 6. Pulsation simple\n    //----------------------------------------------------\n    float pulseMod = sin(t*1.4 + nField*8.0)*0.5 + 0.5;\n    float field = fil * pulseMod;\n\n    //----------------------------------------------------\n    // 7. NoiseOverlay noir et blanc avec OverlayTwist\n    //----------------------------------------------------\n    // CoordonnÃ©es centrÃ©es pour tourbillon\n    vec2 uvOverlay = uv;\n    float angleOverlay = atan(uvOverlay.y, uvOverlay.x);\n    float radiusOverlay = length(uvOverlay);\n    uvOverlay = vec2(cos(angleOverlay + t*OverlayTwist), sin(angleOverlay + t*OverlayTwist)) * radiusOverlay;\n\n    float overlayNoise = softNoise(uvOverlay*12.0 + t*2.0)*2.0 - 1.0; // [-1,1]\n\n    // Lighten/Darken effect en noir et blanc\n    field = overlayNoise > 0.0 ? max(field, field + overlayNoise*NoiseOverlay) : min(field, field + overlayNoise*NoiseOverlay);\n\n    //----------------------------------------------------\n    // 8. Colors + Glow\n    //----------------------------------------------------\n    vec3 col = vec3(0.05, 0.08, 0.12);\n    col.r += field * 1.1;\n    col.g += field * ColorG;\n    col.b += field * ColorB;\n\n    // Glow effect\n    col += pow(field, 2.0) * Glow;\n\n    gl_FragColor = vec4(col, 1.0);\n}\n","Inlets":[{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":0,"Hidden":true,"Custom":"ColorG","Exposed":"colorg","Value":{"Float":0.5},"Init":{"Float":0.5},"Domain":{"Float":{"Min":0.0,"Max":1.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":1,"Hidden":true,"Custom":"ColorB","Exposed":"colorb","Value":{"Float":0.800000011920929},"Init":{"Float":0.800000011920929},"Domain":{"Float":{"Min":0.0,"Max":1.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","Obj