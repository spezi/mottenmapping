c2 uv = gl_FragCoord.xy / RENDERSIZE.xy;\n    uv -= vec2(pos);\n\tuv.x *= RENDERSIZE.x/RENDERSIZE.y;\n\tuv = rotate2d(rotateCanvas *-TAU) * uv;\n\tuv *= zoom;\n        \n    float col = shape(abs(uv), cos((motion * TAU * (TIME*0.05))));\n    \n    if  (invert) col = col *-1.0 + 1.0;\n    \n    gl_FragColor = vec4(vec3(col), 1.0);\n}","Inlets":[{"uuid":"fb27e4cb-ea7f-41e2-ad92-2354498c1b6b","ObjectName":"Inlet","id":0,"Hidden":true,"Custom":"invert","Exposed":"invert","Value":{"Bool":false},"Init":{"Bool":false},"Domain":{"Bool":null}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":1,"Hidden":true,"Custom":"zoom","Exposed":"zoom","Value":{"Float":2.0},"Init":{"Float":2.0},"Domain":{"Float":{"Min":0.25,"Max":20.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":2,"Hidden":true,"Custom":"rotateCanvas","Exposed":"rotatecanvas","Value":{"Float":0.0},"Init":{"Float":0.0},"Domain":{"Float":{"Min":0.0,"Max":1.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":3,"Hidden":true,"Custom":"rotateLines","Exposed":"rotatelines","Value":{"Float":0.0},"Init":{"Float":0.0},"Domain":{"Float":{"Min":0.0,"Max":1.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":4,"Hidden":true,"Custom":"lineThickness","Exposed":"linethickness","Value":{"Float":0.5},"Init":{"Float":0.5},"Domain":{"Float":{"Min":0.10000000149011612,"Max":20.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":5,"Hidden":true,"Custom":"lineLength","Exposed":"linelength","Value":{"Float":1.0},"Init":{"Float":1.0},"Domain":{"Float":{"Min":0.05000000074505806,"Max":10.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":6,"Hidden":true,"Custom":"lines1","Exposed":"lines1","Value":{"Float":10.0},"Init":{"Float":10.0},"Domain":{"Float":{"Min":1.0,"Max":10.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":7,"Hidden":true,"Custom":"lines2","Expose