 45.32);\n    return fract(p.x * p.y);\n}\n\nfloat noise(vec2 p) {\n    vec2 i = floor(p);\n    vec2 f = fract(p);\n    f = f * f * (3.0 - 2.0 * f);\n    \n    float a = hash(i);\n    float b = hash(i + vec2(1.0, 0.0));\n    float c = hash(i + vec2(0.0, 1.0));\n    float d = hash(i + vec2(1.0, 1.0));\n    \n    return mix(mix(a, b, f.x), mix(c, d, f.x), f.y);\n}\n\nfloat fbm(vec2 p) {\n    float value = 0.0;\n    float amplitude = 0.5;\n    \n    for(int i = 0; i < 7; i++) {\n        value += amplitude * noise(p);\n        p *= 2.03;\n        amplitude *= 0.52;\n    }\n    return value;\n}\n\n// Voronoi fÃ¼r organische Zell-Struktur\nvec2 voronoi(vec2 x) {\n    vec2 p = floor(x);\n    vec2 f = fract(x);\n    \n    float minDist = 1.0;\n    vec2 minPoint;\n    \n    for(int j = -1; j <= 1; j++) {\n        for(int i = -1; i <= 1; i++) {\n            vec2 b = vec2(float(i), float(j));\n            vec2 r = b - f + hash(p + b) * vec2(1.0, 1.0);\n            float d = dot(r, r);\n            \n            if(d < minDist) {\n                minDist = d;\n                minPoint = r;\n            }\n        }\n    }\n    \n    return vec2(sqrt(minDist), 0.0);\n}\n\nvoid main() {\n    vec2 uv = isf_FragNormCoord;\n    float t = TIME * windSpeed;\n    \n    // GroÃŸe Windwellen Ã¼ber das ganze Feld\n    float windWave = sin(uv.x * 2.5 + t) * cos(uv.y * 1.8 - t * 0.7);\n    windWave += sin(uv.x * 1.3 - t * 1.2) * 0.7;\n    windWave *= windStrength * 0.15;\n    \n    // UV mit Wind verzerren\n    vec2 distortedUv = uv;\n    distortedUv.x += windWave;\n    distortedUv.y += sin(uv.x * 8.0 + t * 1.5) * windStrength * 0.03;\n    \n    // Basis-Textur mit mehreren Noise-Layern\n    float scale = 25.0 * bladeDetail;\n    float base = fbm(distortedUv * scale);\n    \n    // Feinere Details fÃ¼r Grashalme\n    float detail1 = fbm(distortedUv * scale * 2.2 + vec2(t * 0.3, 0.0));\n    float detail2 = fbm(distortedUv * scale * 4.5 - vec2(0.0, t * 0.2));\n    \n    // Voronoi fÃ¼r organische Clumps/BÃ¼schel\n    vec2 voronoiPattern = voronoi(distortedUv * 12.0 + vec2(t * 0.2, 0.0));\n    float cells = voronoiPattern.x;\n    \n    // Kombiniere Textur-Layer\n    float grassTexture = base * 0.4 + detail1 * 0.35 + detail2 * 0.25;\n    grassTexture = mix(grassTexture, cells, 0.3);\n    \n    // Streaks fÃ¼r Motion Blur / Windrichtung\n    float streaks = fbm(vec2(distortedUv.x * 3.0, distortedUv.y * 45.0 + windWave * 5.0));\n    grassTexture = mix(grassTexture, streaks, 0.4);\n    \n    // Farbpalette - GrÃ¼ntÃ¶ne mit Gelb-Variationen\n    vec3 darkGreen = vec3(0.25, 0.4, 0.2);\n    vec3 midGreen = vec3(0.45, 0.55, 0.3);\n    vec3 lightGreen = vec3(0.6, 0.65, 0.45);\n    vec3 yellow = vec3(0.7, 0.7, 0.4);\n    \n    // Farbe basierend auf Textur\n    vec3 col = mix(darkGreen, midGreen, grassTexture);\n    col = mix(col, lightGreen, smoothstep(0.4, 0.7, grassTexture));\n    col = mix(col, yellow, smoothstep(0.6, 0.9, grassTexture) * colorMix);\n    \n    // Schatten in den Vertiefungen\n    float shadow = smoothstep(0.2, 0.5, grassTexture);\n    col *= 0.5 + shadow * 0.5;\n    \n    // Highlights auf den Spitzen\n    float highlights = smoothstep(0.7, 0.95, grassTexture + windWave);\n    col += highlights * vec3(0.3, 0.3, 0.2);\n    \n    // TiefenunschÃ¤rfe (oben schÃ¤rfer, unten weicher)\n    float depthBlur = uv.y * 0.3;\n    float blurredTexture = fbm(distortedUv * scale * 0.5);\n    col = mix(col, mix(darkGreen, lightGreen, blurredTexture), depthBlur);\n    \n    // Kontrast und Helligkeit\n    col = (col - 0.5) * contrast + 0.5;\n    col *= brightness;\n    \n    // Subtile Farbvariationen\n    float colorNoise = noise(distortedUv * 8.0 + t * 0.1);\n    col += (colorNoise - 0.5) * 0.08;\n    \n    gl_FragColor = vec4(col, 1.0);\n}","Inlets":[{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":0,"Hidden":true,"Custom":"windSpeed","Exposed":"windspeed","Value":{"Float":0.800000011920929},"Init":{"Float":0.800000011920929},"Domai