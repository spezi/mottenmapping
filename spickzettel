# blender sh4lt addon
~/apps/blender-4.5.5-linux-x64
mkdir wheels
cp sh4lt-0.2.0-cp311-cp311-linux_x86_64.whl .
./4.5/python/bin/python3.11 -m pip install ./wheels/sh4lt-0.2.0-cp311-cp311-linux_x86_64.whl


https://docs.blender.org/manual/en/latest/advanced/extensions/python_wheels.html
python -m build --wheel
python setup.py bdist_wheel

# mapping
* blender edit mode selected mesh
* uv unwrap projection bounds
* uv export as svg
* generate isf shader 
python3 svg_to_isf.py low_poly_dragonfly_new.svg --isf
* copy isf shader code and replace in ossia score
* compile -- done

!! svg elements have to be polygons not path to get recognized by svg_to_isf.py

# blender 
normals shif+n alt+n 

#test

# artnet
# All channels to 255
python artnet-test.py --ip 192.168.1.100 --universe 0

# All channels to 128, custom port
python artnet-test.py --ip 192.168.1.100 --port 6454 --universe 1 --value 128

# Blackout (all channels to 0)
python artnet-test.py --ip 192.168.1.100 --universe 0 --value 0

