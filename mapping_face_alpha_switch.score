ULT\": 1.0,\n      \"MIN\": 0.0,\n      \"MAX\": 10.0\n    },\n    {\n      \"NAME\": \"origin\",\n      \"TYPE\": \"point3D\",\n      \"DEFAULT\": [0.0, 0.0, 0.0]\n    },\n    {\n      \"NAME\": \"axis\",\n      \"TYPE\": \"long\",\n      \"VALUES\": [\"X\", \"Y\", \"Z\"],\n      \"DEFAULT\": 1\n    },\n    {\n      \"NAME\": \"displacement_direction\",\n      \"TYPE\": \"long\",\n      \"VALUES\": [\"Normal\", \"Radial\", \"World Y\", \"World XYZ\"],\n      \"DEFAULT\": 0\n    },\n    {\n      \"NAME\": \"falloff\",\n      \"TYPE\": \"float\",\n      \"DEFAULT\": 0.0,\n      \"MIN\": 0.0,\n      \"MAX\": 1.0\n    },\n    {\n      \"NAME\": \"octaves\",\n      \"TYPE\": \"long\",\n      \"DEFAULT\": 3,\n      \"MIN\": 1,\n      \"MAX\": 6\n    },\n    {\n      \"NAME\": \"lacunarity\",\n      \"TYPE\": \"float\",\n      \"DEFAULT\": 2.0,\n      \"MIN\": 1.0,\n      \"MAX\": 4.0\n    },\n    {\n      \"NAME\": \"persistence\",\n      \"TYPE\": \"float\",\n      \"DEFAULT\": 0.5,\n      \"MIN\": 0.0,\n      \"MAX\": 1.0\n    }\n  ]\n}*/\n\n// ============ NOISE FUNCTIONS (from Noise Displacement) ============\n\nfloat noise_hash(vec3 p) {\n    p = fract(p * 0.3183099 + 0.1);\n    p *= 17.0;\n    return fract(p.x * p.y * p.z * (p.x + p.y + p.z));\n}\n\nfloat noise_value(vec3 p) {\n    vec3 i = floor(p);\n    vec3 f = fract(p);\n    f = f * f * (3.0 - 2.0 * f);\n    \n    return mix(mix(mix(noise_hash(i + vec3(0.0, 0.0, 0.0)), \n                       noise_hash(i + vec3(1.0, 0.0, 0.0)), f.x),\n                   mix(noise_hash(i + vec3(0.0, 1.0, 0.0)), \n                       noise_hash(i + vec3(1.0, 1.0, 0.0)), f.x), f.y),\n               mix(mix(noise_hash(i + vec3(0.0, 0.0, 1.0)), \n                       noise_hash(i + vec3(1.0, 0.0, 1.0)), f.x),\n                   mix(noise_hash(i + vec3(0.0, 1.0, 1.0)), \n                       noise_hash(i + vec3(1.0, 1.0, 1.0)), f.x), f.y), f.z);\n}\n\nvec3 noise_grad(vec3 p) {\n    float h = noise_hash(p);\n    vec3 grad = vec3(\n        cos(h * 6.28318530718),\n        sin(h * 6.28318530718),\n        cos(h * 3.14159265359)\n    );\n    return normalize(grad);\n}\n\nfloat noise_perlin(vec3 p) {\n    vec3 i = floor(p);\n    vec3 f = fract(p);\n    vec3 u = f * f * (3.0 - 2.0 * f);\n    \n    return mix(mix(mix(dot(noise_grad(i + vec3(0.0, 0.0, 0.0)), f - vec3(0.0, 0.0, 0.0)),\n                       dot(noise_grad(i + vec3(1.0, 0.0, 0.0)), f - vec3(1.0, 0.0, 0.0)), u.x),\n                   mix(dot(noise_grad(i + vec3(0.0, 1.0, 0.0)), f - vec3(0.0, 1.0, 0.0)),\n                       dot(noise_grad(i + vec3(1.0, 1.0, 0.0)), f - vec3(1.0, 1.0, 0.0)), u.x), u.y),\n               mix(mix(dot(noise_grad(i + vec3(0.0, 0.0, 1.0)), f - vec3(0.0, 0.0, 1.0)),\n                       dot(noise_grad(i + vec3(1.0, 0.0, 1.0)), f - vec3(1.0, 0.0, 1.0)), u.x),\n                   mix(dot(noise_grad(i + vec3(0.0, 1.0, 1.0)), f - vec3(0.0, 1.0, 1.0)),\n                       dot(noise_grad(i + vec3(1.0, 1.0, 1.0)), f - vec3(1.0, 1.0, 1.0)), u.x), u.y), u.z);\n}\n\nfloat noise_fractal(vec3 p, int octaves, float lacunarity, float persistence) {\n    float value = 0.0;\n    float amplitude = 1.0;\n    float frequency = 1.0;\n    float maxValue = 0.0;\n    \n    for (int i = 0; i < octaves && i < 6; i++) {\n        value += noise_perlin(p * frequency) * amplitude;\n        maxValue += amplitude;\n        frequency *= lacunarity;\n        amplitude *= persistence;\n    }\n    \n    return value / maxValue; // Normalize\n}\n\n// ============ HELPER FUNCTIONS ============\n\nfloat calculate_falloff(vec3 pos, vec3 origin, float falloff_amount) {\n    if (falloff_amount <= 0.0) return 1.0;\n    float dist = length(pos - origin);\n    return exp(-dist * falloff_amount * 2.0);\n}\n\n// ============ MAIN PROCESS ============\n\nvoid process_vertex(inout vec3 position, inout vec3 normal, inout vec2 uv, inout vec3 tangent, inout vec4 color)\n{\n    float t = TIME * this_filter.speed;\n    float amp = this_filter.intensity;\n    float freq = this_filter.frequency;\n    vec3 origin = this_filter.origin;\n    vec3 relative_pos = position - origin;\n    float fo = calculate_falloff(position, origin, this_filter.falloff);\n    \n    // 0: Passthrough\n    if (this_filter.effect_select == 0) {\n        return;\n    }\n    \n    // 1: Wave\n    else if (this_filter.effect_select == 1) {\n        float phase;\n        if (this_filter.axis == 0) phase = relative_pos.x * freq + t;\n        else if (this_filter.axis == 1) phase = relative_pos.y * freq + t;\n        else phase = relative_pos.z * freq + t;\n        \n        float displacement = sin(phase) * amp * fo;\n        \n        if (this_filter.displacement_direction == 0) position += normal * displacement;\n        else if (this_filter.displacement_direction == 1) position += normalize(relative_pos + 0.001) * displacement;\n        else if (this_filter.displacement_direction == 2) position.y += displacement;\n        else position += vec3(displacement * 0.5);\n    }\n    \n    // 2: Noise\n    else if (this_filter.effect_select == 2) {\n        vec3 noise_pos = relative_pos * freq + vec3(t * 0.5);\n        float n = noise_fractal(noise_pos, this_filter.octaves, this_filter.lacunarity, this_filter.persistence);\n        float displacement = n * amp * fo;\n        \n        if (this_filter.displacement_direction == 0) position += normal * displacement;\n        else if (this_filter.displacement_direction == 1) position += normalize(relative_pos + 0.001) * displacement;\n        else if (this_filter.displacement_direction == 2) position.y += displacement;\n        else {\n            // World XYZ - different noise for each axis\n            vec3 noise_3d = vec3(\n                noise_fractal(noise_pos + vec3(100.0, 0.0, 0.0), this_filter.octaves, this_filter.lacunarity, this_filter.persistence),\n                noise_fractal(noise_pos + vec3(0.0, 100.0, 0.0), this_filter.octaves, this_filter.lacunarity, this_filter.persistence),\n                noise_fractal(noise_pos + vec3(0.0, 0.0, 100.0), this_filter.octaves, this_filter.lacunarity, this_filter.persistence)\n            );\n            position += noise_3d * amp * fo;\n        }\n    }\n    \n    // 3: Twist\n    else if (this_filter.effect_select == 3) {\n        float twist_amount;\n        if (this_filter.axis == 0) twist_amount = relative_pos.x;\n        else if (this_filter.axis == 1) twist_amount = relative_pos.y;\n        else twist_amount = relative_pos.z;\n        \n        float angle = twist_amount * freq * 0.1 * amp * fo + t * 0.5;\n        float c = cos(angle);\n        float s = sin(angle);\n        \n        vec3 p = relative_pos;\n        if (this_filter.axis == 0) {\n            position = origin + vec3(p.x, p.y * c - p.z * s, p.y * s + p.z * c);\n        } else if (this_filter.axis == 1) {\n            position = origin + vec3(p.x * c - p.z * s, p.y, p.x * s + p.z * c);\n        } else {\n            position = origin + vec3(p.x * c - p.y * s, p.x * s + p.y * c, p.z);\n        }\n    }\n    \n    // 4: Bulge\n    else if (this_filter.effect_select == 4) {\n        float dist = length(relative_pos);\n        float bulge = sin(dist * freq - t) * amp * fo;\n        position = origin + relative_pos * (1.0 + bulge);\n    }\n    \n    // 5: Pinch\n    else if (this_filter.effect_select == 5) {\n        float axis_dist;\n        if (this_filter.axis == 0) axis_dist = abs(relative_pos.x);\n        else if (this_filter.axis == 1) axis_dist = abs(relative_pos.y);\n        else axis_dist = abs(relative_pos.z);\n        \n        float pinch = 1.0 - amp * fo * (1.0 - smoothstep(0.0, 1.0 / freq, axis_dist));\n        \n        vec3 p = relative_pos;\n        if (this_filter.axis == 0) { p.y *= pinch; p.z *= pinch; }\n        else if (this_filter.axis == 1) { p.x *= pinch; p.z *= pinch; }\n        else { p.x *= pinch; p.y *= pinch; }\n        position = origin + p;\n    }\n    \n    // 6: Ripple\n    else if (this_filter.effect_select == 6) {\n        vec2 planar;\n        if (this_filter.axis == 0) planar = relative_pos.yz;\n        else if (this_filter.axis == 1) planar = relative_pos.xz;\n        else planar = relative_pos.xy;\n        \n        float dist = length(planar);\n        float ripple = sin(dist * freq - t * 2.0) * amp * fo;\n        ripple *= exp(-dist * this_filter.falloff * 0.5);\n        \n        if (this_filter.axis == 0) position.x += ripple;\n        else if (this_filter.axis == 1) position.y += ripple;\n        else position.z += ripple;\n    }\n    \n    // 7: Explode\n    else if (this_filter.effect_select == 7) {\n        // Use noise for per-vertex variation\n        vec3 noise_pos = position * 10.0;\n        float variation = noise_value(noise_pos) * 0.5 + 0.5;\n        \n        // Pulsing explosion\n        float explosion = amp * (0.5 + 0.5 * sin(t)) * fo;\n        \n        // Direction: along normal with some noise variation\n        vec3 dir = normalize(normal + (vec3(\n            noise_value(noise_pos + vec3(17.0, 0.0, 0.0)),\n            noise_value(noise_pos + vec3(0.0, 31.0, 0.0)),\n            noise_value(noise_pos + vec3(0.0, 0.0, 47.0))\n        ) - 0.5) * 0.5);\n        \n        position += dir * explosion * variation;\n    }\n}","Inlets":[{"uuid":"f2ab26ea-415d-45a2-bfbc-2968c7c92a33","ObjectName":"Inlet","id":0,"Hidden":false,"Custom":"Geometry In","Exposed":"geometry in"},{"uuid":"485680cc-b8b9-4a01-acc7-3e8334bdc016","ObjectName":"Inlet","id":1,"Hidden":true,"Custom":"effect_select","Exposed":"effect_select","Value":{"Int":2},"Init":{"Int":0},"Domain":{"Int":{"Values":[0,1,2,3,4,5,6,7]}},"Values":[["Passthrough",{"Int":0}],["Wave",{"Int":1}],["Noise",{"Int":2}],["Twist",{"Int":3}],["Bulge",{"Int":4}],["Pinch",{"Int":5}],["Ripple",{"Int":6}],["Explode",{"Int":7}]]},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":2,"Hidden":true,"Custom":"intensity","Exposed":"intensity","Value":{"Float":1.3015625476837158},"Init":{"Float":0.30000001192092896},"Domain":{"Float":{"Min":0.0,"Max":2.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":3,"Hidden":true,"Custom":"frequency","Exposed":"frequency","Value":{"Float":5.753880023956299},"Init":{"Float":3.0},"Domain":{"Float":{"Min":0.10000000149011612,"Max":20.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":4,"Hidden":true,"Custom":"speed","Exposed":"speed","Value":{"Float":1.3411458730697632},"Init":{"Float":1.0},"Domain":{"Float":{"Min":0.0,"Max":10.0}}},{"uuid":"377e8205-b442-4d54-8832-3761def522b2","ObjectName":"Inlet","id":5,"Hidden":true,"Custom":"origin","Exposed":"origin","Value":{"Vec3f":[0.0,3.009999990463257,-4.860000133514404]},"Init":{"Vec3f":[0.0,0.0,0.0]},"Domain":{"Vec3f":{"Min":[-100.0,-100.0,-100.0],"Max":[100.0,100.0,100.0],"Values":[[],[],[]]}}},{"uuid":"485680cc-b8b9-4a01-acc7-3e8334bdc016","ObjectName":"Inlet","id":6,"Hidden":true,"Custom":"axis","Exposed":"axis","Value":{"Int":1},"Init":{"Int":1},"Domain":{"Int":{"Values":[0,1,2]}},"Values":[["X",{"Int":0}],["Y",{"Int":1}],["Z",{"Int":2}]]},{"uuid":"485680cc-b8b9-4a01-acc7-3e8334bdc016","ObjectName":"Inlet","id":7,"Hidden":true,"Custom":"displacement_direction","Exposed":"displacement_direction","Value":{"Int":3},"Init":{"Int":0},"Domain":{"Int":{"Values":[0,1,2,3]}},"Values":[["Normal",{"Int":0}],["Radial",{"Int":1}],["World Y",{"Int":2}],["World XYZ",{"Int":3}]]},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":8,"Hidden":true,"Custom":"falloff","Exposed":"falloff","Value":{"Float":0.5742971897125244},"Init":{"Float":0.0},"Domain":{"Float":{"Min":0.0,"Max":1.0}}},{"uuid":"485680cc-b8b9-4a01-acc7-3e8334bdc016","ObjectName":"Inlet","id":9,"Hidden":true,"Custom":"octaves","Exposed":"octaves","Value":{"Int":3},"Init":{"Int":3},"Domain":{"Int":{"Values":[0,1,2]}},"Values":[["0",{"Int":0}],["1",{"Int":1}],["2",{"Int":2}]]},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":10,"H