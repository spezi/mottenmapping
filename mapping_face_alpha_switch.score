= (iGlobalTime);\n    z *= mat2(int(TWISTS),int(SEGMENTS),int(SKEW),int(PITCH)); //mat2(0.707,-0.707,0.707,0.707)*2./0.283;\n    // Drawing time.\n    fragColor = vec4(SHARPNESS*SHARPNESSSCALE*sin(z.x)*sin(z.y));\n\n}\n\nvoid main(void) {\n    mainImage(gl_FragColor, gl_FragCoord.xy);\n}","Inlets":[{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":0,"Hidden":true,"Custom":"TWISTS","Exposed":"twists","Value":{"Float":7.335241794586182},"Init":{"Float":5.0},"Domain":{"Float":{"Min":-50.0,"Max":50.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":1,"Hidden":true,"Custom":"SEGMENTS","Exposed":"segments","Value":{"Float":2.3352417945861816},"Init":{"Float":-5.0},"Domain":{"Float":{"Min":-50.0,"Max":50.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":2,"Hidden":true,"Custom":"SKEW","Exposed":"skew","Value":{"Float":-0.9980915784835815},"Init":{"Float":5.0},"Domain":{"Float":{"Min":-50.0,"Max":50.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":3,"Hidden":true,"Custom":"PITCH","Exposed":"pitch","Value":{"Float":-15.998091697692871},"Init":{"Float":5.0},"Domain":{"Float":{"Min":-50.0,"Max":50.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":4,"Hidden":true,"Custom":"SHARPNESS","Exposed":"sharpness","Value":{"Float":1.0},"Init":{"Float":1.0},"Domain":{"Float":{"Min":0.0,"Max":1.0}}},{"uuid":"af2b4fc3-aecb-4c15-a5aa-1c573a239925","ObjectName":"Inlet","id":5,"Hidden":true,"Custom":"SHARPNESSSCALE","Exposed":"sharpnessscale","Value":{"Float":1000.0},"Init":{"Float":1000.0},"Domain":{"Float":{"Min":0.0,"Max":1000.0}}}],"Outlets":[{"uuid":"f1c71046-b754-49a5-8e66-d01374773dfc","ObjectName":"Outlet","id":1,"Hidden":false}]},{"uuid":"74ca45ff-92c9-44a0-8f1a-754dea05ee1b","ObjectName":"gfxProcess","id":29,"Metadata":{"ScriptingName":"MakeMyNoiseTheP-Flow2","Comment":"","Color":"Transparent1","Label":"","Touched":true},"Duration":10584000000,"Heigh